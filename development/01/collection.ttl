    tbox:hasAlternativeLabel "Provenance creation"@en ;
    tbox:hasDefinition "A digitization activity meant to create provenance metadata to track the agent responsible for data creation, the time of data creation, and the primary source of the data"@en ;
    tbox:hasBroaderConcept :digitization-activity ;
    tbox:hasRelatedConcept <https://www.wikidata.org/wiki/Q105100083> ;
    tbox:hasCreator <https://orcid.org/0000-0002-0799-1527> ;
    tbox:hasCreationDate "2024-01-12T00:00:00Z"^^xsd:datetime ;
    tbox:hasMotivation "Added to represent the activity of creating provenance metadata for some digital records within a digitization workflow"@en ;
    tbox:hasSource <https://doi.org/10.1016/j.daach.2023.e00309> .

:upload a tbox:Concept ;
    tbox:hasPreferredLabel "Upload"@en ;
    tbox:hasAlternativeLabel "3D model upload"@en ;
    tbox:hasDefinition "A digitization meant to transfer digital 3D models from a local device or storage location to a Web-based framework"@en ;
    tbox:hasBroaderConcept :digitization-activity ;
    tbox:hasRelatedConcept <https://www.wikidata.org/wiki/Q7126699> ;
    tbox:hasCreator <https://orcid.org/0000-0002-0799-1527> ;
    tbox:hasCreationDate "2024-01-12T00:00:00Z"^^xsd:datetime ;
    tbox:hasMotivation "Added to represent the activity of uploading some digital object on a publication platform within a digitization workflow"@en ;
    tbox:hasSource <https://doi.org/10.1016/j.daach.2023.e00309> .

:digitization-activity a tbox:Concept ;
    tbox:hasPreferredLabel "Digitization activity"@en ;
    tbox:hasDefinition "An activity carried out within a digitization workflow"@en ;
    tbox:hasRelatedConcept <https://www.wikidata.org/wiki/Q843958> ;
    tbox:hasCreator <https://orcid.org/0000-0002-0799-1527> ;
    tbox:hasCreationDate "2024-01-12T00:00:00Z"^^xsd:datetime ;
    tbox:hasMotivation "Added to represent the 3D modelling activity carried out on some digital files through the use of software within a digitization workflow"@en ;
    tbox:hasSource <https://doi.org/10.1016/j.daach.2023.e00309> .
