@prefix : <http://purl.org/changes/diva/development/01/data/> .
@prefix tbox: <http://purl.org/changes/diva/development/01/schema/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:acquisition a tbox:Concept ;
    tbox:hasPreferredLabel "Acquisition"@en ;
    tbox:hasAlternativeLabel "Data capture"@en ;
    tbox:hasDefinition "A digitization activity meant to capture analogue materials and acquire their digital representation"@en ;
    tbox:hasBroaderConcept :digitization-activity ;
    tbox:hasRelatedConcept <https://www.wikidata.org/wiki/Q1172399> ;
    tbox:hasCreator <https://orcid.org/0000-0002-0799-1527> ;
    tbox:hasCreationDate "2024-01-12T00:00:00Z"^^xsd:datetime ;
    tbox:hasMotivation "Added to represent the data acquisition activity carried out on an object (e.g. a sculpture, a book, a conversation, etc.) through the use of hardware and/or software within a digitization workflow"@en ;
    tbox:hasSource <https://doi.org/10.1016/j.daach.2023.e00309> .

:processing a tbox:Concept ;
    tbox:hasPreferredLabel "Processing"@en ;
    tbox:hasAlternativeLabel "Data processing"@en ;
    tbox:hasDefinition "A digitization activity meant to manipulate digital files in order to facilitate further operations on them"@en ;
    tbox:hasBroaderConcept :digitization-activity ;
    tbox:hasRelatedConcept <https://www.wikidata.org/wiki/Q6661985> ;
    tbox:hasCreator <https://orcid.org/0000-0002-0799-1527> ;
    tbox:hasCreationDate "2024-01-12T00:00:00Z"^^xsd:datetime ;
    tbox:hasMotivation "Added to represent the data processing activity carried out on some digital files through the use of software within a digitization workflow"@en ;
    tbox:hasSource <https://doi.org/10.1016/j.daach.2023.e00309> .

:modelling a tbox:Concept ;
    tbox:hasPreferredLabel "Modelling"@en ;
    tbox:hasAlternativeLabel "3D modelling"@en ;
    tbox:hasDefinition "A digitization activity meant to bring together digital files in order to create a fully-fledged 3D model"@en ;
    tbox:hasBroaderConcept :digitization-activity ;
    tbox:hasRelatedConcept <https://www.wikidata.org/wiki/Q568742> ;
    tbox:hasCreator <https://orcid.org/0000-0002-0799-1527> ;
    tbox:hasCreationDate "2024-01-12T00:00:00Z"^^xsd:datetime ;
    tbox:hasMotivation "Added to represent the 3D modelling activity carried out on some digital files through the use of software within a digitization workflow"@en ;
    tbox:hasSource <https://doi.org/10.1016/j.daach.2023.e00309> .

:optimization a tbox:Concept ;
    tbox:hasPreferredLabel "Optimization"@en ;
    tbox:hasAlternativeLabel "3D model optimization"@en ;
    tbox:hasDefinition "A digitization activity meant to optimise a digital 3D model for specific purposes or use cases"@en ;
    tbox:hasBroaderConcept :digitization-activity ;
    tbox:hasRelatedConcept <https://www.wikidata.org/wiki/Q1156793> ;
    tbox:hasCreator <https://orcid.org/0000-0002-0799-1527> ;
    tbox:hasCreationDate "2024-01-12T00:00:00Z"^^xsd:datetime ;
    tbox:hasMotivation "Added to represent the 3D model optimization activity carried out on some 3D model through the use of software within a digitization workflow"@en ;
    tbox:hasSource <https://doi.org/10.1016/j.daach.2023.e00309> .

:export a tbox:Concept ;
    tbox:hasPreferredLabel "Export"@en ;
    tbox:hasAlternativeLabel "Data export"@en ;
    tbox:hasDefinition "A digitization activity meant to convert the digital files into a specific format"@en ;
    tbox:hasBroaderConcept :digitization-activity ;
    tbox:hasRelatedConcept <https://www.wikidata.org/wiki/Q59154708> ;
    tbox:hasCreator <https://orcid.org/0000-0002-0799-1527> ;
    tbox:hasCreationDate "2024-01-12T00:00:00Z"^^xsd:datetime ;
    tbox:hasMotivation "Added to represent the data export activity carried out in a software used to edit some digital artifact within a digitization workflow"@en ;
    tbox:hasSource <https://doi.org/10.1016/j.daach.2023.e00309> .

:descriptive-metadata-creation a tbox:Concept ;
    tbox:hasPreferredLabel "Descriptive metadata creation"@en ;
    tbox:hasAlternativeLabel "Metadata creation"@en ;
    tbox:hasDefinition "A digitization activity meant to create structured metadata describing the content and context of the digital files"@en ;
    tbox:hasBroaderConcept :digitization-activity ;
    tbox:hasRelatedConcept <https://www.wikidata.org/wiki/Q105100083> ;
    tbox:hasCreator <https://orcid.org/0000-0002-0799-1527> ;
    tbox:hasCreationDate "2024-01-12T00:00:00Z"^^xsd:datetime ;
    tbox:hasMotivation "Added to represent the activity of creating descriptive metadata for some physical and/or digital objects within a digitization workflow"@en ;
    tbox:hasSource <https://doi.org/10.1016/j.daach.2023.e00309> .

:provenance-metadata-creation a tbox:Concept ;
